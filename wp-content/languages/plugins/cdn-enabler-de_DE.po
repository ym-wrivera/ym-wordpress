msgid "Exclusions"
msgstr "Ausschlüsse"

#: inc/cdn_enabler_settings.class.php:151
msgid "Enter the exclusions (directories or extensions) separated by"
msgstr "Eingabe der Ausschlüsse (Verzeichnisse oder Dateiendungen) separiert durch"

#: inc/cdn_enabler.class.php:123
msgid "Settings"
msgstr "Einstellungen"

#: inc/cdn_enabler.class.php:177
msgid "CDN Enabler is optimized for WordPress %s. Please disable the plugin or upgrade your WordPress installation (recommended)."
msgstr "Der CDN Enabler ist optimiert für WordPress %s. Bitte Plugin deaktivieren oder WordPress aktualisieren (empfohlen)."

#: inc/cdn_enabler_settings.class.php:90
msgid "CDN Enabler Settings"
msgstr "CDN Enabler Einstellungen"

#: inc/cdn_enabler_settings.class.php:94
msgid "Combine CDN Enabler with <b><a href=\"%s\">%s</a></b> for even faster WordPress performance."
msgstr "Kombiniere den CDN Enabler mit <b><a href=\"%s\">%s</a></b> für noch bessere WordPress Performance."

#: inc/cdn_enabler_settings.class.php:106
msgid "CDN URL"
msgstr "CDN URL"

#: inc/cdn_enabler_settings.class.php:115
msgid "Enter the CDN URL without trailing"
msgstr "Eingabe der CDN URL ohne abschliessenden"

#: inc/cdn_enabler_settings.class.php:123
msgid "Included Directories"
msgstr "Eingeschlossene Verzeichnisse"

#: inc/cdn_enabler_settings.class.php:129
msgid "Default: <code>wp-content,wp-includes</code>"
msgstr "Standard: <code>wp-content,wp-includes</code>"

#: inc/cdn_enabler_settings.class.php:133
msgid "Assets in these directories will be pointed to the CDN URL. Enter the directories separated by"
msgstr "Dateien in diesen Verzeichnissen werden mit der CDN URL verknüpft. Eingabe der Verzeichnisse erfolgt separiert durch "

#: inc/cdn_enabler_settings.class.php:147
msgid "Default: <code>.php</code>"
msgstr "Standard: <code>.php</code>"

#: inc/cdn_enabler_settings.class.php:159
msgid "Relative Path"
msgstr "Relativer Pfad"

#: inc/cdn_enabler_settings.class.php:165
msgid "Enable CDN for relative paths (default: enabled)."
msgstr "Aktiviert das CDN für relative Pfade (Standard: aktiviert)."

#: inc/cdn_enabler_settings.class.php:173
msgid "CDN HTTPS"
msgstr "CDN HTTPS"

#: inc/cdn_enabler_settings.class.php:179
msgid "Enable CDN for HTTPS connections (default: disabled)."
msgstr "Aktiviert das CDN für HTTPS Verbindungen (Standard: deaktiviert)."

